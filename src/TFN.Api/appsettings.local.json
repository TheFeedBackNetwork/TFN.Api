{
  "Logging": {
    "StorageAccountConnectionString": "UseDevelopmentStorage=true;",
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "ApplicationInsights": {
    "InstrumentationKey": ""
  },
  "Storage": {
    "StorageAccountConnectionString": "UseDevelopmentStorage=true;"
  },
  "TranscodeBitrate": "128",
  "Authorization": {
    "Authority": "http://localhost:5000",
    "Audience": "http://localhost:5000/resources"
  },
  "DocumentDb": {
    "EndpointUri": "{TFN_DB_URL}",
    "Key": "{TFN_DB_KEY}",
    "DatabaseName": "tfn-db"
  },
  "Messaging": {
    "KeyBaseUrl": "http://localhost:5000/account",
    "Email": {
      "Username": "{POSTMARK_USERNAME}",
      "Sender": "team@thefeedbacknetwork.com",
      "Password": "{POSTMARK_PASSWORD}",
      "SmtpHost": "smtp.postmarkapp.com",
      "SmtpPort": 587
    }
  }
}